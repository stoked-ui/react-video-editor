{
	"name": "react-video-editor",
	"author": "Brian Stoker <b@stokedconsulting.com>",
	"description": "React client side video editing component leveraging react-player, ffmpeg, mui, and a bunch of other crap.",
	"keywords": [
		"video editor",
		"react",
		"react video editor"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/stokedconsulting/react-video-editor"
	},
	"license": "MIT",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"setup": "git init && husky install && npx playwright install",
		"format": "prettier src/**/*.{ts,tsx} --write",
		"lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
		"dev": "vite",
		"debug": "DEBUG=vite:* vite",
		"storybook": "storybook dev -p 6006",
		"storybook:setup": "node node_modules/@storybook/addon-styling/bin/postinstall.js",
		"storybook:build": "storybook build",
		"test": "vitest run src/ && playwright test",
		"test:unit": "vitest src/",
		"test:unit:coverage": "vitest --coverage src/",
		"test:e2e": "playwright test",
		"test:e2e:report": "playwright show-report",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"commitlint": "commitlint --edit",
		"commitizen": "exec < /dev/tty && git cz --hook || true",
		"clean": "rimraf --glob **/pnpm-lock.yaml && rimraf --glob **/node_modules && rimraf --glob **/dist && rimraf --glob **/.build",
		"chrome-cors": "run-script-os",
		"chrome-cors:macos": "bash src/scripts/chrome/cors-mac.sh",
		"chrome-cors:windows": "src/scripts/chrome/cors-windows.bat",
		"chrome-nocors": "run-script-os",
		"chrome-nocors:macos": "bash src/scripts/chrome/nocors-mac.sh",
		"chrome-nocors:windows": "src/scripts/chrome/nocors-windows.bat"
	},
