{
  "name": "@stokedui/video-editor",
  "author": "Brian Stoker",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stokedui/video-editor"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "pnpm ffmpeg && vite",
    "ffmpeg": "mkdir -p ./public/lib/ffmpeg/core/dist && cp ./node_modules/@ffmpeg/core-mt/dist/esm/* ./public/lib/ffmpeg/core/dist",
    "build": "tsc --p ./tsconfig-build.json && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "@mui/icons-material": "^5.2.3",
    "@mui/material": "^5.2.3",
    "@mui/styled-engine": "^5.2.3",
    "@mui/system": "^5.2.3",
    "@stokedui/file-list": "workspace:*",
    "@stokedui/media-selector": "workspace:*",
    "@stokedui/timeline": "workspace:*",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@emotion/types": "^0.7.0",
    "@types/node": "^20.14.8",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.1",
    "esbuild-plugin-react-virtualized": "^1.0.4",
